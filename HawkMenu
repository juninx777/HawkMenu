-- Scripts:

local function YHUKT_fake_script() -- BotaoPrender.LocalScript 
	local script = Instance.new('LocalScript', BotaoPrender)

	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local prenderJogadorEvent = ReplicatedStorage:WaitForChild("PrenderJogadorEvent")
	
	local botaoPrender = script.Parent
	local nomePreso = script.Parent.Parent.NomePreso
	local tempoPreso = script.Parent.Parent.TempoPreso
	local motivoPreso = script.Parent.Parent.MotivoPreso
	
	function prenderJogador()
		local jogador = nomePreso.Text
		local tempo = tonumber(tempoPreso.Text)
		local motivo = motivoPreso.Text
	
		if jogador ~= "" and tempo and motivo ~= "" then
			print("Enviando evento para o servidor...")
			prenderJogadorEvent:FireServer(jogador, tempo, motivo)
		else
			print("Por favor, preencha todos os campos corretamente.")
		end
	end
	
	botaoPrender.MouseButton1Click:Connect(prenderJogador)
	
end
coroutine.wrap(YHUKT_fake_script)()
local function TOGC_fake_script() -- NoclipButton.LocalScript 
	local script = Instance.new('LocalScript', NoclipButton)

	local button = script.Parent
	local player = game.Players.LocalPlayer
	local char = player.Character or player.CharacterAdded:Wait()
	
	local cam = workspace.CurrentCamera
	local uis = game:GetService("UserInputService")
	
	local flying = false
	local bodyVelocity
	local speed = 100
	
	local function toggleFlight()
		flying = not flying
		if flying then
			if not bodyVelocity then
				bodyVelocity = Instance.new("BodyVelocity", char.PrimaryPart)
				bodyVelocity.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
				bodyVelocity.Velocity = Vector3.new(0, 0, 0)
			end
			button.Text = "Desativar NoClip"
		else
			if bodyVelocity then
				bodyVelocity:Destroy()
				bodyVelocity = nil
			end
			button.Text = "NoClip"
		end
	end
	
	button.MouseButton1Click:Connect(toggleFlight)
	
	uis.InputBegan:Connect(function(input, isProcessed)
		if isProcessed then return end
		if input.KeyCode == Enum.KeyCode.Q then
			toggleFlight()
		end
	end)
	
	while true do
		if flying then
			if bodyVelocity then
				bodyVelocity.Velocity = Vector3.new(0, 0, 0) 
	
				if uis:IsKeyDown(Enum.KeyCode.W) then
					bodyVelocity.Velocity = cam.CFrame.LookVector * speed
				elseif uis:IsKeyDown(Enum.KeyCode.S) then
					bodyVelocity.Velocity = cam.CFrame.LookVector * -speed
				elseif uis:IsKeyDown(Enum.KeyCode.A) then
					bodyVelocity.Velocity = cam.CFrame.RightVector * -speed
				elseif uis:IsKeyDown(Enum.KeyCode.D) then
					bodyVelocity.Velocity = cam.CFrame.RightVector * speed
				end
			end
		end
		wait(0.1)
	end
	
end
coroutine.wrap(TOGC_fake_script)()
local function ATDLAVA_fake_script() -- BotaoRevistar.LocalScript 
	local script = Instance.new('LocalScript', BotaoRevistar)

	local scriptParent = script.Parent
	local nomeRevistar = scriptParent.Parent.NomeRevistar
	local itensLabel = scriptParent.Parent.Itens
	
	local function listarItens()
		local playerName = nomeRevistar.Text
	
		if playerName ~= "" then
			local targetPlayer = game.Players:FindFirstChild(playerName)
	
			if targetPlayer then
				local inventory = {}
				local playerInventory = targetPlayer:FindFirstChild("Backpack")
	
				if playerInventory then
					for _, item in pairs(playerInventory:GetChildren()) do
						table.insert(inventory, item.Name)
					end
				end
	
				if #inventory > 0 then
					itensLabel.Text = table.concat(inventory, ", ")
				else
					itensLabel.Text = "O jogador está limpo."
				end
			else
				itensLabel.Text = "Jogador não encontrado."
			end
		else
			itensLabel.Text = "Digite o nome do indivíduo."
		end
	end
	
	script.Parent.MouseButton1Click:Connect(listarItens)
end
coroutine.wrap(ATDLAVA_fake_script)()
local function LWBD_fake_script() -- RemoverItens.LocalScript 
	local script = Instance.new('LocalScript', RemoverItens)

	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local remoteEvent = ReplicatedStorage:WaitForChild("RemoverItens")
	
	script.Parent.MouseButton1Click:Connect(function()
		local playerName = script.Parent.Parent.NomeRevistar.Text
	
		if playerName ~= "" then
			
			remoteEvent:FireServer(playerName)
		else
			warn("Por favor, insira um nome de jogador.")
		end
	end)
	
end
coroutine.wrap(LWBD_fake_script)()
local function JDQYR_fake_script() -- HawkMenu.TrocaAba 
	local script = Instance.new('LocalScript', HawkMenu)

	local botaoTroll = script.Parent.Painel.aba2.TrollsButton
	local botaoInicio = script.Parent.Painel.aba2.InicioButton
	local botaoVisual = script.Parent.Painel.aba2.VisualButton
	local botaoVeiculos = script.Parent.Painel.aba2.VeiculoButton
	local botaoArmas = script.Parent.Painel.aba2.ArmasButton
	local botaoDinheiro = script.Parent.Painel.aba2.DinheiroButton
	
	local interface = script.Parent.interface
	local inicio = script.Parent.Inicio
	local pesquisa = script.Parent.Pesquisa
	local revistar = script.Parent.Revistar
	
	local function fecharOutrasGui()
	    interface.Visible = false
	    inicio.Visible = false
	    pesquisa.Visible = false
	    revistar.Visible = false
	end
	
	botaoTroll.MouseButton1Click:Connect(function()
	    if botaoTroll.Visible then
	        fecharOutrasGui()
	        interface.Visible = true
	    end
	end)
	
	botaoInicio.MouseButton1Click:Connect(function()
	    if botaoInicio.Visible then
	        fecharOutrasGui()
	        inicio.Visible = true
	    end
	end)
	
	botaoVisual.MouseButton1Click:Connect(function()
	    if botaoVisual.Visible then
	        fecharOutrasGui()
	        pesquisa.Visible = true
	    end
	end)
	
	botaoVeiculos.MouseButton1Click:Connect(function()
	    if botaoVeiculos.Visible then
	        fecharOutrasGui()
	        revistar.Visible = true
	    end
	end)
	
end
coroutine.wrap(JDQYR_fake_script)()
